{
  "defaultSeverity": "error",
  "extends": ["tslint-react", "tslint-config-airbnb", "tslint-config-prettier"],
  "exclude": [
    "node_modules",
    "modules/**/node_modules",
    "modules/**/dist/*",
    "modules/**/ts-tmp/*",
    "docs/**/*"
  ],
  "rules": {
    "import-name": [
      true,
      {
        "react": "React",
        "canvasKitReactCore": "canvas",
        "canvasKitReactButton": "Button",
        "canvasKitReactTable": "Table",
        "READMEMd": "README",
        "canvasAccentIconsWeb": "accentIcons",
        "canvasAppletIconsWeb": "appletIcons",
        "canvasSystemIconsWeb": "systemIcons",
        "classnames": "classNames",
        "icon": "CanvasIcon"
      }
    ],
    "semicolon": [true, "never"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "strict-boolean-expressions": [true, "allow-string", "allow-null-union"]
  }
}
